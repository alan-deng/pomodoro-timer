{
  "name": "Test ext",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [ "background.js" ]
  },
  "permissions": ["storage", "activeTab", "tabs", "notifications", "http://*/*", "https://*/*"],

  
  "browser_action": {
    "default_popup": "popup.html"
 },

  "icons": {
    "16": "/images/alarm-clock-16.png",
    "32": "/images/alarm-clock-32.png",
    "48": "/images/alarm-clock-48.png",
    "128": "/images/alarm-clock-128.png"
  },
  "options_page": "options.html",
  "web_accessible_resources": ["template.html"]
}
